<%= form_with model: @test do |form| %>

  <%= render 'shared/form_errors', resource: @test %>
  <p>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </p>

  <p>
    <%= form.label :level %>
    <%= form.number_field :level %>
  </p>

  <p>
    <%= form.label :category_id %>
    <%= form.collection_select :category_id, Category.all, :id, :title, prompt: true %>
  </p>

  <p><%= form.submit %></p>
<% end %>

<p> <%= link_to 'To tests', tests_path %></p>